{
  "name": "Fictorial Light (Green)",
  "colors": {
    "editor.background": "#f9fcec",
    "editor.foreground": "#4f5d14",
    "statusBar.background": "#e7ebd4",
    "statusBar.foreground": "#4f5d14",
    "statusBar.debuggingBackground": "#93ab26",
    "statusBar.debuggingForeground": "#dbe6a8",
    "activityBar.background": "#eaefd7",
    "activityBar.foreground": "#4f5d14",
    "tab.inactiveBackground": "#f9fcec",
    "tab.inactiveForeground": "#a4ad80",
    "tab.activeBackground": "#dbe6a8",
    "editorLineNumber.foreground": "#93ab26",
    "editor.lineHighlightBackground": "#dbe6a8",
    "sideBar.background": "#f0f4df",
    "sideBar.foreground": "#4f5d14",
    "list.activeSelectionIconForeground": "#4f5d14",
    "list.activeSelectionBackground": "#93ab26",
    "list.activeSelectionForeground": "#dbe6a8",
    "list.inactiveSelectionBackground": "#dbe6a8",
    "list.inactiveSelectionForeground": "#4f5d14",
    "editorGroupHeader.tabsBackground": "#f2f6de",
    "titleBar.activeBackground": "#f0f4df",
    "titleBar.inactiveBackground": "#f9fcec"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ab2647"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#4a9b27"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#FF5733"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#FF5733"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#4a9b27"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#4a9b27"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#4f5d14"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#4a9b27"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4a9b27"
      }
    }
  ]
}
